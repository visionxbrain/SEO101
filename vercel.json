{
  "buildCommand": "cd url-checker-app && npm install && npm run build",
  "outputDirectory": "url-checker-app/dist",
  "framework": "vite",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}