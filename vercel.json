{
  "version": 2,
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "url-checker-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "url-checker-app/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "url-checker-app/$1"
    }
  ]
}